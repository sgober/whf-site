{
  "name": "whf-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ignore-pattern .gitignore",
    "lint:fix": "eslint . --ignore-pattern .gitignore --fix",
    "format": "prettier --check './**/*.{js,jsx,scss,md,json}' --config ./.prettierrc --ignore-path .gitignore",
    "format:fix": "prettier --write './**/*.{js,jsx,scss,md,json}' --config ./.prettierrc --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vite-jsconfig-paths": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  }
}
